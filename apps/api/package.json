{
  "name": "todo-api",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "build": "tsup src/server.ts --format cjs",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist",
    "dev": "tsup src/server.ts --format cjs --watch --onSuccess \"node dist/server.js\"",
    "lint": "tsc --noEmit && TIMING=1 eslint \"src/**/*.ts*\"",
    "start": "node dist/server.js"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.17.1",
    "joi": "^17.13.1",
    "mongoose": "^8.4.1",
    "morgan": "^1.10.0"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.0",
    "@types/cors": "^2.8.10",
    "@types/express": "^4.17.12",
    "@types/jest": "^26.0.22",
    "@types/morgan": "^1.9.2",
    "@types/node": "^15.12.2",
    "@types/supertest": "^2.0.12",
    "eslint": "^7.32.0",
    "eslint-config-custom-server": "*",
    "jest": "^26.6.3",
    "supertest": "^6.2.4",
    "tsconfig": "*",
    "tsup": "^6.2.3",
    "typescript": "^4.8.3"
  }
}
